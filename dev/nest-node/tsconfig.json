{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2017",
    "sourceMap": true,
    "outDir": "../../src/node/nest",
    "baseUrl": "./",
    "incremental": false,
    "skipLibCheck": true,
  },
  "exclude": ["node_modules", "dist"]
}
